#include <dt-bindings/gpio/gpio.h>

/ {
	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,arm920t";
			reg = <0>;
		};
	};

	aliases {
		gpio-f = &gpiof;
	};
	
	soc {
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		
		gpiof: gpio@56000050 {
			compatible = "samsung,s3c24xx-gpio";
			gpio-controller;
			reg = <0x50000000 0x08>;
			#gpio-cells = <2>;
			label = "GPIO_F";
			status = "ok";
		};
	};
};